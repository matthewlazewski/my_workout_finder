<%= form_for([@category, @workout]) do |f| %>
    <%= display_category(f) %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :intensity %>
    <%= f.number_field :intensity %><br>
    <%= f.label :duration %>
    <%= f.number_field :duration %>
    <br>
    <%= f.submit %>
<% end %>